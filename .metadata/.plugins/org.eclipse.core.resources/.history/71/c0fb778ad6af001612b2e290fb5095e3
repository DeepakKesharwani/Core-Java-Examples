	public static void main(String[] args) {
		
		List<Integer> l1 = new ArrayList<>() ;
		
		for(int i=0;i<=10;i++){
			l1.add(i);
		}
		
		System.out.println(l1);
		
		for(Integer eachNumber:l1){
			if(eachNumber%2==0){
				
			}
		}
		

	}

}
